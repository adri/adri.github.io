{
  "private": true,
  "name": "minimal-blog",
  "description": "Typography driven, feature-rich blogging theme with minimal aesthetics. Includes tags/categories support and extensive features for code blocks such as live preview, line numbers, and line highlighting.",
  "version": "1.2.0",
  "author": "LekoArts <hello@lekoarts.de>",
  "license": "MIT",
  "starter-name": "gatsby-starter-minimal-blog",
  "scripts": {
    "build": "gatsby build --prefix-paths",
    "deploy": "GATSBY_EXPERIMENTAL_PAGE_BUILD_ON_DATA_CHANGES=true gatsby build --prefix-paths --log-pages && gh-pages-clean && gh-pages -d public",
    "develop": "gatsby develop",
    "develop:cypress": "cross-env CYPRESS_SUPPORT=y yarn develop",
    "build:cypress": "cross-env CYPRESS_SUPPORT=y yarn build",
    "start": "gatsby develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean"
  },
  "dependencies": {
    "@aengusm/gatsby-theme-brain": "^1.0.13",
    "@lekoarts/gatsby-theme-minimal-blog": "^2.4.0",
    "@reach/portal": "^0.10.5",
    "algoliasearch": "^4.4.0",
    "gatsby": "^2.20.36",
    "gatsby-plugin-algolia": "^0.11.2",
    "gatsby-plugin-disqus": "^1.2.2",
    "gatsby-plugin-force-trailing-slashes": "^1.0.4",
    "gatsby-plugin-ipfs": "^2.0.2",
    "gatsby-plugin-manifest": "^2.2.3",
    "gatsby-plugin-offline": "^2.2.4",
    "gatsby-plugin-sitemap": "^2.2.19",
    "gatsby-plugin-webpack-bundle-analyser-v2": "^1.1.10",
    "gatsby-remark-external-links": "^0.0.4",
    "gatsby-remark-line-breaks": "^1.0.0",
    "gh-pages": "^3.1.0",
    "prismjs": "^1.27.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-instantsearch-dom": "^6.7.0"
  },
  "devDependencies": {
    "cross-env": "^5.2.0"
  }
}
